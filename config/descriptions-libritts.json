{
  "dataset": {
    "train": "data/libritts-description-train-2.csv",
    "test": "data/libritts-description-test-2.csv",
    "val": "data/libritts-description-val-2.csv",
    "preprocessing": {
      "allowed_chars": "!'(),.:;? \\-abcdefghijklmnopqrstuvwxyz",
      "expand_abbreviations": true,
      "end_token": "^",
      "silence": 512,
      "trim": false,
      "num_mels": 80,
      "cache": false,
      "sample_rate": 24000
    }
  },
  "training": {
    "lr": 0.000125,
    "batch_size": 8,
    "weight_decay": 0.000001,
    "precision": "16-mixed",
    "name": "descriptions-libritts",
    "float32_matmul_precision": "high",
    "stopping_val_loss_threshold": null,
    "description_embeddings": true,
    "args": {
      "max_steps": 100000
    }
  },
  "model": {
    "scheduler_milestones": [],
    "args": {
      "prenet_dim": 256,
      "att_rnn_dim": 1024,
      "att_dim": 128,
      "rnn_hidden_dim": 1024,
      "postnet_dim": 512,
      "dropout": 0.5,
      "char_embedding_dim": 512,
      "encoder_kernel_size": 5,
      "description_embeddings": true,
      "description_embeddings_dim": 1024
    }
  },
  "extensions": {
    "speaker_tokens": {
      "active": true,
      "num_speakers": 562
    },
    "controls": {
      "active": false
    },
    "descriptions": {
      "bert_embeddings": true
    }
  }
}